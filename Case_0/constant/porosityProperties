FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      porosityProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

porosityModel JensenEtAl2014;

porosityEnabled true;

JensenEtAl2014Coeffs
{
    alpha 500;
    beta 2;
    gamma_p 0.34;
    d50 0.0159;
    KC 68.68; //se calcula usando parametros de oleaje y velocidad obtenida del movimiento libre (t = 0.7, n=0.5, um=0.78)
}


